{
    "name": "Web Notifier",
    "version": "1.0.0",
    "description": "Chrome extension to notify users of safe whitelisted sites.",
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": {
            "32": "images/redicon38.png"
        }
    }

}